{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "lint": {},
    "test": {},
    "typecheck": {},
    "clean": {
      "cache": false,
      "outputs": [
        "node_modules",
        "dist",
        ".next"
      ]
    }
  }
}
